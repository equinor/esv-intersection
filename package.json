{
  "name": "@equinor/esv-intersection",
  "version": "0.1.2-alpha",
  "description": "Intersection component package with testing and automatic documentation.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "copyfiles images/* docs && copyfiles .nojekyll docs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier": "prettier --write --config .prettierrc **/*.ts "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/esv-intersection.git"
  },
  "keywords": [
    "videx",
    "heappurple",
    "intersection",
    "typescript"
  ],
  "contributors": [
    "Tom Kristian Tjemsland",
    "Brynjulf Risbakken"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/esv-intersection/issues"
  },
  "homepage": "https://github.com/equinor/esv-intersection#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/html": "^5.3.14",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "eslint-config-prettier": "^6.10.0",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "storybook-dark-mode": "^0.3.0",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@equinor/videx-math": "^1.0.12",
    "@equinor/videx-vector2": "^1.0.44",
    "@types/d3": "^5.7.2",
    "curve-interpolator": "^1.0.6",
    "d3-axis": "^1.0.12",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-zoom": "^1.8.3",
    "pixi.js": "^5.2.1"
  }
}
