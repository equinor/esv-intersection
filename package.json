{
  "name": "@equinor/esv-intersection",
  "version": "3.0.0-beta.4",
  "description": "Intersection component package with testing and automatic documentation.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "prepub-beta": "npm run build",
    "pub-beta": "npm publish --tag=beta --access=public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src test",
    "test": "tsc --project tsconfig.test.json && jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs_out",
    "docs": "typedoc --out docs_out src",
    "postdocs": "copyfiles images/* docs_out && copyfiles .nojekyll docs_out",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier": "prettier --write --config .prettierrc \"**/*.ts\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/esv-intersection.git"
  },
  "keywords": [
    "videx",
    "heappurple",
    "intersection",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/esv-intersection/issues"
  },
  "homepage": "https://github.com/equinor/esv-intersection#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/html": "^6.5.13",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.2.3",
    "@types/mock-raf": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-jest": "^29.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.7",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "mock-raf": "^1.0.1",
    "pixi.js": "^6.5.8",
    "pixi-dashed-line": "^1.4.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "storybook-dark-mode": "^1.1.2",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!d3)"
    ]
  },
  "peerDependencies": {
    "pixi.js": "^6.5.8",
    "pixi-dashed-line": "^1.4.2"
  },
  "dependencies": {
    "@equinor/videx-math": "^1.1.0",
    "@equinor/videx-vector2": "^1.0.44",
    "curve-interpolator": "2.0.8",
    "d3-array": "^3.2.0",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-zoom": "^3.0.0"
  }
}
