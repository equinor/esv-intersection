{
  "name": "@equinor/esv-intersection",
  "version": "3.0.0-beta.6",
  "description": "Intersection component package with testing and automatic documentation.",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "browser": "./dist/index.umd.js"
  },
  "files": [
    "dist/*",
    "src/*",
    "!test/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "vite build",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "prepub-beta": "npm run build",
    "pub-beta": "npm publish --tag=beta --access=public",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src test",
    "test": "tsc --project tsconfig.test.json && jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs_out",
    "docs": "typedoc --out docs_out src",
    "postdocs": "copyfiles images/* docs_out && copyfiles .nojekyll docs_out",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prettier": "prettier --write --config .prettierrc \"**/*.ts\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/esv-intersection.git"
  },
  "keywords": [
    "videx",
    "heappurple",
    "intersection",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/esv-intersection/issues"
  },
  "homepage": "https://github.com/equinor/esv-intersection#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-storysource": "^7.0.0-beta.15",
    "@storybook/html-vite": "^7.0.0-beta.15",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.2.4",
    "@types/mock-raf": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-jest": "^29.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.8",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "mock-raf": "^1.0.1",
    "pixi.js": "^7.0.5",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "storybook": "^7.0.0-beta.15",
    "storybook-dark-mode": "^2.0.4",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.22",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-dts": "^1.7.1"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!d3)"
    ]
  },
  "peerDependencies": {
    "pixi.js": "^7.0.5"
  },
  "dependencies": {
    "@equinor/videx-math": "^1.1.0",
    "@equinor/videx-vector2": "^1.0.44",
    "curve-interpolator": "2.0.8",
    "d3-array": "^3.2.1",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-zoom": "^3.0.0"
  }
}
