{
  "name": "@equinor/esv-intersection",
  "version": "3.0.4",
  "description": "Intersection component package with testing and automatic documentation.",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "browser": "./dist/index.umd.js"
  },
  "files": [
    "dist/*",
    "src/*",
    "!test/*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "vite build",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "prepub-beta": "npm run build",
    "pub-beta": "npm publish --tag=beta --access=public",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx src test",
    "lint:tsc": "tsc -b",
    "lint": "run-p lint:tsc lint:eslint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "predocs": "rimraf docs_out",
    "docs": "typedoc --out docs_out src",
    "postdocs": "copyfiles images/* docs_out && copyfiles .nojekyll docs_out",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/esv-intersection.git"
  },
  "keywords": [
    "videx",
    "heappurple",
    "intersection",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/esv-intersection/issues"
  },
  "homepage": "https://github.com/equinor/esv-intersection#readme",
  "devDependencies": {
    "@storybook/addon-storysource": "^7.0.7",
    "@storybook/html-vite": "^7.0.18",
    "@types/d3": "^7.4.0",
    "@types/mock-raf": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mock-raf": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "pixi.js": "^7.2.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "storybook": "^7.0.7",
    "storybook-dark-mode": "^3.0.0",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.4",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.0",
    "vitest-canvas-mock": "^0.2.2"
  },
  "peerDependencies": {
    "pixi.js": "^7.1.0"
  },
  "dependencies": {
    "@equinor/videx-math": "^1.1.0",
    "@equinor/videx-vector2": "^1.0.44",
    "curve-interpolator": "3.1.1",
    "d3-array": "^3.2.3",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0"
  }
}
